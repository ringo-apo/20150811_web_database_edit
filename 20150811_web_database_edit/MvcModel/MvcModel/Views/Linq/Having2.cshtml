@model IOrderedQueryable<IGrouping<MvcModel.Models.CategoryEnum, MvcModel.Models.Article>>

@{
  ViewBag.Title = "Having2";
}

<h2>Having2</h2>

@foreach (var group in Model)
{
  <dt>@group.Key</dt>
  <dd>
    @foreach (var article in group)
    {
      <p><a href="@article.Url">@article.Title</a></p>
    }
  </dd>
}

